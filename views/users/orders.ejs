// Get references to the button, the add address form, and the arrow span
const toggleAddressFormButton = document.getElementById('toggleAddressForm');
const addAddressForm = document.getElementById('addAddressForm');
const arrowSpan = toggleAddressFormButton.querySelector('.arrow');

// Add event listener to the button to toggle the add address form
toggleAddressFormButton.addEventListener('click', function() {
  // Toggle the display style of the add address form
  if (addAddressForm.style.display === 'block') {
    addAddressForm.style.display = 'none'; // Hide the add address form
    arrowSpan.textContent = '\u25BC'; // Change arrow to down
  } else {
    addAddressForm.style.display = 'block'; // Show the add address form
    arrowSpan.textContent = '\u25B2'; // Change arrow to up
  }
});
</script>


