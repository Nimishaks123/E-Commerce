<script>
    document.addEventListener('DOMContentLoaded', function() {
    const couponForm = document.getElementById('couponForm');
    const couponSelect = document.getElementById('couponSelect');
    const applyButton = document.getElementById('applyButton');
    const removeButton = document.getElementById('removeButton');
    const messageDiv = document.getElementById('couponMessage');
    let subPrice = parseFloat(document.getElementById('subtotal').textContent);
    let discountElement = document.getElementById('discount');
    let totalPriceElement = document.getElementById('totalPrice');
    let discountRow = document.getElementById('discount-row');

    console.log("Initial Subtotal: ", subPrice);

    // Function to fetch userCoupon from the server
    async function fetchUserCoupon() {
        try {
            const response = await fetch('/coupon/used');
            if (!response.ok) {
                throw new Error('Failed to fetch user coupon');
            }
            const data = await response.json();
            console.log('Fetched user coupon: ', data);
            return data.couponId || ''; // Return userCoupon or empty string if not available
        } catch (error) {
            console.error('Error fetching user coupon:', error);
            return ''; // Return empty string in case of error
        }
    }

    // Function to handle form submission
    async function handleFormSubmission(event) {
        event.preventDefault(); // Prevent the form from submitting the traditional way

        const userCoupon = await fetchUserCoupon();
        const couponCode = couponSelect.value;
        const selectedOption = couponSelect.options[couponSelect.selectedIndex];
        const offerPercentage = parseFloat(selectedOption.getAttribute('data-offer-percentage'));

        console.log("User coupon: ", userCoupon);
        console.log("Selected coupon: ", couponCode);
        console.log("Offer Percentage: ", offerPercentage);

        // Check if the user is reapplying the same coupon
        if (couponCode === userCoupon) {
            Swal.fire({
                icon: 'error',
                title: 'Oops...',
                text: 'You have already applied this coupon.'
            });
            return;
        }

        // Proceed with applying the coupon
        try {
            const response = await fetch('/coupon/apply', {
                method: 'POST',
                headers: {
                    'Content-Type': 'application/json',
                },
                body: JSON.stringify({ couponId: couponCode })
            });
            const data = await response.json();
            console.log('Coupon apply response: ', data);

            if (data.success) {
                messageDiv.textContent = 'Coupon applied successfully!';
                messageDiv.style.color = 'green';

                // Show the remove button
                applyButton.style.display = 'none';
                removeButton.style.display = 'block';

                const discountAmount = (subPrice * offerPercentage) / 100;
                const totalPrice = subPrice - discountAmount;

                discountElement.textContent = discountAmount.toFixed(2);
                totalPriceElement.textContent = totalPrice.toFixed(2);
                discountRow.style.display = 'table-row';

                console.log("Discount: ", discountAmount);
                console.log("Total Price: ", totalPrice);
            } else {
                messageDiv.textContent = data.message || 'Failed to apply coupon.';
                messageDiv.style.color = 'red';
            }
        } catch (error) {
            console.error('Error:', error);
            messageDiv.textContent = 'An error occurred while applying the coupon.';
            messageDiv.style.color = 'red';
        }
    }

    // Function to handle coupon removal
    function handleCouponRemoval() {
        fetch('/coupon/remove', {
            method: 'PUT',
            headers: {
                'Content-Type': 'application/json',
            },
            body: JSON.stringify({ couponId: couponSelect.value })
        })
        .then(response => response.json())
        .then(data => {
            if (data.success) {
                messageDiv.textContent = 'Coupon removed successfully!';
                messageDiv.style.color = 'green';

                // Hide the remove button
                applyButton.style.display = 'block';
                removeButton.style.display = 'none';

                // Reset discount and total price
                discountElement.innerText = '0.00';
                totalPriceElement.innerText = subPrice.toFixed(2);
                discountRow.style.display = 'none';
            } else {
                messageDiv.textContent = data.message || 'Failed to remove coupon.';
                messageDiv.style.color = 'red';
            }
        })
        .catch(error => {
            console.error('Error:', error);
            messageDiv.textContent = 'An error occurred while removing the coupon.';
            messageDiv.style.color = 'red';
        });
    }

    // Attach event listener to the form submission
    if (couponForm) {
        couponForm.addEventListener('submit', handleFormSubmission);
    }

    // Attach event listener to the remove button
    removeButton.addEventListener('click', function() {
        Swal.fire({
            title: 'Are you sure?',
            text: "You won't be able to revert this!",
            icon: 'warning',
            showCancelButton: true,
            confirmButtonColor: '#3085d6',
            cancelButtonColor: '#d33',
            confirmButtonText: 'Yes, remove it!'
        }).then((result) => {
            if (result.isConfirmed) {
                handleCouponRemoval();
            }
        });
    });
    })

</script>
    
