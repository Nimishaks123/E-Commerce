                                                        <% if (item.productId) { %>
                                                            <a href="/productDetails?productId=<%= item.productId._id %>">
                                                                <%= item.productId.name %>
                                                            </a>
                                                            <p class="font-xs"><%= item.productId.description %><br></p>
                                                            <% if (item.productId.isDelete) { %>
                                                                <p class="text-danger">This product is no longer available.</p>
                                                            <% } %>
                                                        <% } else { %>
                                                            Product not available
                                                        <% } %>
                                                    </h5>
                                                </td>
                                                <td class="size" data-title="size">
                                                    <span id="size"><%= item.size %> </span>
                                                    <% if (item.productId) { %>
                                                        <input id="productId" type="hidden" value="<%= item.productId._id %>">
                                                    <% } %>
                                                </td>
                                                <td class="price" data-title="Price">
                                                    <% if (item.productId) { %>
                                                        <span>₹<%= item.productId.promo_Price %> </span>
                                                    <% } else { %>
                                                        N/A
                                                    <% } %>
                                                </td>
                                                <td>
                                                    <div>
                                                        <input type="number" class="qty-val" value="<%= item.quantity %>" data-max-quantity="<%= item.productId ? item.productId.countInStock : 0 %>" data-product-id="<%= item.productId ? item.productId._id : '' %>" style="width: 70px;">
                                                        <div class="error-message" style="color: red; display: none;">Quantity exceeds available stock.</div>
                                                    </div>
                                                </td>
                                                <td class="text-right" data-title="Subtotal">
                                                    <p class="total-price" data-product-id="<%= item.productId ? item.productId._id : '' %>">₹<%= item.totalPrice %> </p>
                                                </td>
                                                <td class="action" data-title="Remove">
                                                    <% if (item.productId) { %>
                                                        <a href="#" onclick="confirmRemove('<%= item.productId._id %>','<%= item.size %>')" class="text-muted">
                                                            <i class="fi-rs-trash"></i>
                                                        </a>
                                                    <% } else { %>
                                                        <a href="#" class="text-muted" onclick="return false;">
