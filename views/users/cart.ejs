<script>
document.addEventListener('DOMContentLoaded', function () {
    const quantityInputs = document.querySelectorAll('.qty-val');

    quantityInputs.forEach(input => {
        input.addEventListener('input', function () {
            const maxQuantity = parseInt(this.dataset.maxQuantity, 10);
            const currentQuantity = parseInt(this.value, 10);
            const errorMessage = this.nextElementSibling;

            if (isNaN(currentQuantity) || currentQuantity <= 0) {
                errorMessage.textContent = "Quantity must be a positive number.";
                errorMessage.style.display = "block";
                this.value = ''; // Clear the input value if error
            } else if (currentQuantity > maxQuantity) {
                errorMessage.textContent = "Quantity exceeds available stock.";
                errorMessage.style.display = "block";
                this.value = ''; // Clear the input value if error
            } else {
                errorMessage.style.display = "none";
                updateSubtotalAndTotal(this);
                updateQuantity(this.dataset.productId, this.value, this.closest('tr').querySelector('.price').textContent.replace('₹', '').trim());
                console.log(this.closest('tr').querySelector('.price').textContent.replace('₹', '').trim());
            }
        });
    });

    function updateSubtotalAndTotal(input) {
        const productId = input.dataset.productId;
        const currentQuantity = parseInt(input.value, 10);
        const productRow = input.closest('tr');
        const price = parseFloat(productRow.querySelector('.price span').textContent.replace('₹', ''));
        const subtotalElement = productRow.querySelector('.total-price');
        const newSubtotal = price * currentQuantity;

        subtotalElement.textContent = `₹${newSubtotal.toFixed(2)}`;

        let total = 0;
        document.querySelectorAll('.total-price').forEach(subtotal => {
            total += parseFloat(subtotal.textContent.replace('₹', ''));
        });

        document.getElementById('total-amount').textContent = `${total.toFixed(2)}`;
    }

    function updateQuantity(productId, quantity, price) {
    console.log(price);
        console.log(productId,quantity,price);
        fetch('/updateCartQuantity', {
            method: 'POST',
            headers: {
                'Content-Type': 'application/json'
            },
            body: JSON.stringify({ productId, quantity, price })
        })
        .then(response => response.json())
        .then(data => {
            if (data.success) {
                const totalAmount= document.getElementById('total-amount').textContent
                const updateTotal = Number(totalAmount)+50
                document.getElementById('total-amount').textContent = updateTotal;

                console.log('Quantity updated successfully');
            } else {
                console.error('Failed to update quantity');
            }
        })
        .catch(error => {
            console.error('Error:', error);
        });
    }

    window.confirmRemove = function (productId, size) {
        Swal.fire({
            title: 'Are you sure?',
            text: "You won't be able to revert this!",
            icon: 'warning',
            showCancelButton: true,
            confirmButtonColor: '#3085d6',
            cancelButtonColor: '#d33',
            confirmButtonText: 'Yes, remove it!'
        }).then((result) => {
            if (result.isConfirmed) {
                fetch('/removeCart', {
                    method: 'DELETE',
                    headers: {
                        'Content-Type': 'application/json'
                    },
                    body: JSON.stringify({ productId, size })
                })
                .then(response => response.json())
                .then(data => {
                    if (data.success) {
                        // Remove the item row from the table
                        

                        // Update the total amount
                        updateTotal();

                        Swal.fire(
                            'Removed!',
                            'The item has been removed from your cart.',
                            'success'
                        );
                        location.reload();
                    } else {
                        console.error('Failed to remove item from cart');
                    }
                })
                .catch(error => {
                    console.error('Error:', error);
                });
            }
        });
    };

    function updateTotal() {
        let total = 0;
        document.querySelectorAll('.total-price').forEach(subtotal => {
            total += parseFloat(subtotal.textContent.replace('₹', ''));
        });

        document.getElementById('total-amount').textContent = `${total.toFixed(2)}`;
    }
});

</script>


